{
  "name": "Adaptarte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:build": "npm run android:bundle && cd android && gradlew assembleDebug && find . -type d -regex '.*/res/drawable-.*dpi' -exec rm -rf {} ; && cd ..",
    "android:clean": "cd android && gradlew clean && gradlew build && cd ..",
    "format": "prettier --config .prettierrc.js --write src/**/*.{ts,tsx}",
    "ios": "react-native run-ios",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^17.4.2",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/auth": "^17.4.2",
    "@react-native-firebase/firestore": "^17.4.2",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@realm/react": "^0.4.3",
    "firebase": "^9.19.1",
    "react": "18.2.0",
    "react-native": "0.71.1",
    "react-native-date-picker": "^4.2.10",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-swipe-gestures": "^1.0.5",
    "realm": "^11.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.34",
    "@types/react-native": "^0.71.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-estidlore": "github:estidlore/eslint-config-estidlore",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "0.74.1",
    "prettier": "^2.8.7",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
